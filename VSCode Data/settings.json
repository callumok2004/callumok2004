{
  "[css]": {
    "editor.defaultFormatter": "sibiraj-s.vscode-scss-formatter",
    "editor.formatOnSave": false
  },
  "[javascript]": {
    "editor.defaultFormatter": "vscode.typescript-language-features"
  },
  "[scss]": {
    "editor.defaultFormatter": "sibiraj-s.vscode-scss-formatter"
  },
  "[typescript]": {
    "editor.defaultFormatter": "vscode.typescript-language-features"
  },
  "[typescriptreact]": {
    "editor.defaultFormatter": "vscode.typescript-language-features"
  },
  "files.autoGuessEncoding": true,
  "discord.detailsEditing": "üìÅ {file_name} ‚Ä¢ {dir_name} ‚Ä¢ {workspace}",
  "discord.lowerDetailsEditing": "üìù Line: {current_line}/{total_lines} ‚Ä¢ Col: {current_column}",
  "editor.bracketPairColorization.enabled": true,
  "editor.cursorBlinking": "expand",
  "editor.cursorSmoothCaretAnimation": "on",
  "editor.cursorWidth": 5,
  "editor.fontFamily": "'Fira Code'",
  "editor.fontLigatures": true,
  "editor.inlineSuggest.enabled": true,
  "editor.linkedEditing": true,
  "editor.minimap.maxColumn": 100,
  "editor.minimap.scale": 2,
  "editor.minimap.showSlider": "always",
  "editor.suggestSelection": "first",
  "editor.tabSize": 2,
  "git.enableSmartCommit": true,
  "indentRainbow.colorOnWhiteSpaceOnly": false,
  "javascript.updateImportsOnFileMove.enabled": "always",
  "php.validate.executablePath": "C:/xampp/php/php.exe",
  "security.workspace.trust.untrustedFiles": "open",
  "terminal.integrated.defaultProfile.windows": "PowerShell",
  "terminal.integrated.fontFamily": "FiraCode Nerd Font",
  "terminal.integrated.profiles.windows": {
    "Command Prompt": {
      "args": [],
      "icon": "terminal-cmd",
      "path": [
        "${env:windir}\\Sysnative\\cmd.exe",
        "${env:windir}\\System32\\cmd.exe"
      ]
    },
    "Git Bash": {
      "source": "Git Bash"
    },
    "PowerShell": {
      "icon": "terminal-powershell",
      "source": "PowerShell"
    },
    "Windows PowerShell": {
      "path": "C:\\Program Files\\PowerShell\\7\\pwsh.exe"
    }
  },
  "terminal.integrated.scrollback": 10000,
  "vsintellicode.modify.editor.suggestSelection": "automaticallyOverrodeDefaultValue",
  "window.title": "${activeEditorShort}${separator}${rootName} ‚Ä¢ ZARP Gaming",
  "workbench.iconTheme": "material-icon-theme",
  "github.copilot.enable": {
    "*": true,
    "plaintext": true,
    "markdown": false,
    "scminput": false,
    "yaml": false,
    "typescriptreact": true,
    "php": true,
    "glua": true
  },
  "files.associations": {
    ".env": "code-text-binary"
  },
  "liveshare.connectionMode": "relay",
  "glualint.linter": "C:\\Users\\callu\\OneDrive\\Documents\\glualint\\glualint.exe",
  "glualint.run": "manual",
  "[dart]": {
    "editor.formatOnSave": true,
    "editor.formatOnType": true,
    "editor.rulers": [
      80
    ],
    "editor.selectionHighlight": false,
    "editor.suggest.snippetsPreventQuickSuggestions": false,
    "editor.suggestSelection": "first",
    "editor.tabCompletion": "onlySnippets",
    "editor.wordBasedSuggestions": "off"
  },
  "explorer.fileNesting.patterns": {
    "*.ts": "${capture}.js",
    "*.js": "${capture}.js.map, ${capture}.min.js, ${capture}.d.ts",
    "*.jsx": "${capture}.js",
    "*.tsx": "${capture}.ts",
    "tsconfig.json": "tsconfig.*.json",
    "package.json": "package-lock.json, yarn.lock",
    "*.d.ts": "${capture}.d.ts",
    ".env.*": ".env.${capture}"
  },
  "explorer.fileNesting.enabled": true,
  "gitlens.detectNestedRepositories": true,
  "js/ts.implicitProjectConfig.target": "ES2022",
  "discord.detailsIdling": "https://callumdev.xyz",
  "discord.lowerDetailsIdling": "https://zarpgaming.com",
  "discord.smallImage": "GitHub - @callumok2004",
  "discord.detailsDebugging": "üß™(DBG) {file_name} ‚Ä¢ {dir_name} ‚Ä¢ {workspace}",
  "editor.stickyScroll.enabled": true,
  "window.commandCenter": true,
  "git.confirmSync": false,
  "emmet.excludeLanguages": [
    "markdown",
    "php"
  ],
  "intelephense.format.enable": false,
  "intelephense.maxMemory": 2048,
  "intelephense.completion.fullyQualifyGlobalConstantsAndFunctions": true,
  "intelephense.diagnostics.embeddedLanguages": false,
  "git.autofetch": true,
  "editor.accessibilitySupport": "off",
  "php.debug.executablePath": "c:/xampp/php/php.exe",
  "intelephense.runtime": "C:\\Program Files\\nodejs\\node.exe",
  "intelephense.completion.maxItems": 200,
  "editor.insertSpaces": false,
  "[python]": {
    "editor.formatOnType": true
  },
  "workbench.editorAssociations": {
    "*.lua": "default",
    "*.jpg": "imagePreview.previewEditor"
  },
  "diffEditor.maxComputationTime": 0,
  "workbench.tree.indent": 8,
  "workbench.tree.renderIndentGuides": "always",
  "workbench.view.alwaysShowHeaderActions": true,
  "search.showLineNumbers": true,
  "scm.alwaysShowRepositories": true,
  "scm.providerCountBadge": "auto",
  "extensions.ignoreRecommendations": true,
  "gitlens.views.repositories.showIncomingActivity": true,
  "diffEditor.ignoreTrimWhitespace": false,
  "editor.unicodeHighlight.invisibleCharacters": false,
  "diffEditor.experimental.showEmptyDecorations": true,
  "diffEditor.experimental.showMoves": true,
  "diffEditor.hideUnchangedRegions.enabled": true,
  "haskell.manageHLS": "PATH",
  "window.zoomLevel": -0.4,
  "workbench.colorCustomizations": {
    "statusBar.background": "#2333cc96",
    "statusBar.foreground": "#ffffff",
    "statusBar.noFolderBackground": "#212121",
    "statusBar.noFolderForeground": "#fff",
    "statusBar.debuggingBackground": "#ff0000",
    "statusBar.debuggingForeground": "#fff",
    "statusBar.debuggingBorder": "#ff0000",
    "statusBarItem.hoverBackground": "#1AffFF39",
    "statusBarItem.remoteForeground": "#fff",
    "statusBarItem.errorBackground": "#ff0000",
    "statusBarItem.errorForeground": "#fff",
    "statusBarItem.warningBackground": "#ff3000",
    "statusBarItem.warningForeground": "#fff",
  },
  "workbench.colorTheme": "Min Darker",
  "search.maxResults": 250000,
  "todo-tree.regex.regex": "(//|#|<!--|;|/\\*|^|^[ \\t]*(-|\\d+.)|--)\\s*($TAGS)",
  "Lua.hint.await": false,
  "git.openRepositoryInParentFolders": "never",
  "php.completion.namingConvention": "CamelCase",
  "php.highlight-todo.enable": false,
  "php.problems.excludeGitIgnore": true,
  "vscode-mysql.enableTelemetry": false,
  "redhat.telemetry.enabled": false,
  "extensions.trustedPublishers": [
    "aeschli",
    "astonedpenguin",
    "bmewburn",
    "eamodio",
    "gruntfuggly",
    "icrawl",
    "mcright",
    "mhutchie",
    "mrmlnc",
    "naumovs",
    "oderwat",
    "pkief",
    "shardulm94",
    "sibiraj-s",
    "st0pien",
    "sumneko",
    "venner",
    "wakatime",
    "yoavbls",
    "zainchen"
  ],
  "security.promptForLocalFileProtocolHandling": false,
  "Lua.diagnostics.globalsRegex": [
    "^.*$"
  ],
  "Lua.runtime.nonstandardSymbol": [
    "//",
    "||",
    "!",
    "!=",
    "continue",
    "/**/",
    "&&"
  ],
  "Lua.completion.enable": true,
  "Lua.workspace.preloadFileSize": 500000,
  "Lua.diagnostics.disable": [
    "err-comment-prefix",
    "redundant-parameter",
    "undefined-global",
    "err-c-long-comment",
    "duplicate-set-field",
    "redundant-return-value",
    "err-esc"
  ],
  "Lua.runtime.special": {
    "include": "dofile",
    "IncludeCS": "dofile"
  },
  "Lua.runtime.version": "Lua 5.1",
  "Lua.workspace.maxPreload": 500000,
  "Lua.workspace.useGitIgnore": false,
  "intelephense.environment.includePaths": [
    "."
  ],
  "intelephense.files.maxSize": 10000000,
  "Lua.completion.callSnippet": "Both",
  "Lua.completion.displayContext": 1,
  "Lua.completion.keywordSnippet": "Both",
  "Lua.hint.semicolon": "Disable",
  "github.copilot.nextEditSuggestions.enabled": false,
  "security.allowedUNCHosts": [
    "pi"
  ],
  "github.copilot.selectedCompletionModel": "",
  "Lua.codeLens.enable": true,
  "Lua.completion.autoRequire": false,
  "Lua.format.enable": false,
  "Lua.semantic.enable": false,
  "Lua.semantic.annotation": false,
  "Lua.semantic.variable": false
}
