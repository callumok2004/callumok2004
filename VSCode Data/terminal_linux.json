{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "blocks": [
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "type": "git",
          "style": "powerline",
          "powerline_symbol": "\uE0B0",
          "foreground": "#b2cee3",
          "background": "#196678",
          "properties": {
            "fetch_stash_count": true,
            "fetch_upstream_icon": true,
            "template": "{{ .UpstreamIcon }}{{ .HEAD }}{{ if gt .StashCount 0 }} \uF692 {{ .StashCount }}{{ end }}"
          }
        },
        {
          "type": "node",
          "style": "powerline",
          "powerline_symbol": "\uE0B0",
          "foreground": "#000000",
          "background": "#88ce76",
          "properties": {
            "prefix": " \uE718 "
          }
        },
        {
          "type": "shell",
          "style": "powerline",
          "powerline_symbol": "\uE0B0",
          "foreground": "#ffffff",
          "background": "#203248",
          "properties": {
            "prefix": " \uFCB5 "
          }
        },
        {
          "type": "root",
          "style": "powerline",
          "powerline_symbol": "\uE0B0",
          "foreground": "#ffffff",
          "background": "#ffff66"
        },
        {
          "type": "executiontime",
          "style": "diamond",
          "foreground": "#ffffff",
          "background": "#5f26c9",
          "leading_diamond": "",
          "trailing_diamond": "î‚´",
          "properties": {
            "always_enabled": true,
            "prefix": " \ufbab ",
            "postfix": "\u2800"
          }
        }
      ]
    },
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "type": "text",
          "style": "diamond",
          "leading_diamond": "\ue0c7\u2593",
          "foreground": "#000000",
          "background": "#ffea00",
          "properties": {
            "text": "",
            "postfix": "",
            "prefix": ""
          }
        },
        {
          "type": "kubectl",
          "style": "powerline",
          "powerline_symbol": "",
          "foreground": "#000000",
          "background": "#ffea00",
          "properties": {
            "prefix": " \uFD31",
            "template": " {{.Context}}{{if .Namespace}} :: {{.Namespace}}{{end}}"
          }
        },
        {
          "type": "az",
          "style": "powerline",
          "powerline_symbol": "\uE0B0",
          "foreground": "#000000",
          "background": "#008AD7",
          "properties": {
            "prefix": " \uFD03 "
          }
        }
      ]
    },
    {
      "type": "prompt",
      "alignment": "left",
      "newline": true,
      "segments": [
        {
          "type": "os",
          "style": "plain",
          "leading_diamond": "\ue0c7\u2593 ",
          "foreground": "#26C6DA",
          "properties": {
            "windows": "\ue70f",
            "postfix": "<#ffea00> | </>"
          }
        },
        {
          "type": "path",
          "style": "powerline",
          "trailing_diamond": "\ue0b0",
          "foreground": "#fafafa",
          "background": "#2f2f2f",
          "properties": {
            "style": "full",
            "prefix": ""
          }
        }
      ]
    },
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "type": "text",
          "style": "plain",
          "foreground": "#ffea00",
          "properties": {
            "text": "\u276F"
          }
        }
      ]
    }
  ],
  "final_space": true
}
