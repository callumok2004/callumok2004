{
  "[css]": {
    "editor.defaultFormatter": "sibiraj-s.vscode-scss-formatter",
    "editor.formatOnSave": false
  },
  "[javascript]": {
    "editor.defaultFormatter": "ms-vsliveshare.vsliveshare"
  },
  "[scss]": {
    "editor.defaultFormatter": "sibiraj-s.vscode-scss-formatter"
  },
  "[typescript]": {
    "editor.defaultFormatter": "vscode.typescript-language-features"
  },
  "[typescriptreact]": {
    "editor.defaultFormatter": "vscode.typescript-language-features"
  },
  "deepscan.enable": true,
  "deepscan.ignorePatterns": [
    "**/node_modules/**"
  ],
  "deepscan.ignoreRules": [
    "ASSIGN_SAME_VALUE",
    "MISMATCHED_TYPE_OF_ARG"
  ],
  "discord.detailsEditing": "üìÅ {file_name} ‚Ä¢ {dir_name} ‚Ä¢ {workspace}",
  "discord.lowerDetailsEditing": "üìù Line: {current_line}/{total_lines} ‚Ä¢ Col: {current_column}",
  "editor.bracketPairColorization.enabled": true,
  "editor.cursorBlinking": "expand",
  "editor.cursorSmoothCaretAnimation": true,
  "editor.cursorWidth": 5,
  "editor.fontFamily": "'Fira Code'",
  "editor.fontLigatures": true,
  "editor.formatOnSave": true,
  "editor.inlineSuggest.enabled": true,
  "editor.linkedEditing": true,
  "editor.minimap.maxColumn": 100,
  "editor.minimap.scale": 2,
  "editor.minimap.showSlider": "always",
  "editor.suggestSelection": "first",
  "editor.tabSize": 2,
  "files.autoSave": "onWindowChange",
  "git.enableSmartCommit": true,
  "importSorter.generalConfiguration.sortOnBeforeSave": true,
  "importSorter.importStringConfiguration.quoteMark": "double",
  "importSorter.importStringConfiguration.tabSize": 2,
  "indentRainbow.colorOnWhiteSpaceOnly": false,
  "javascript.updateImportsOnFileMove.enabled": "always",
  "php.validate.executablePath": "C:/Users/callu/Documents/php-8.0.9-nts-Win32-vs16-x64/php.exe",
  "security.workspace.trust.untrustedFiles": "open",
  "tabnine.experimentalAutoImports": true,
  "terminal.integrated.defaultProfile.windows": "Windows PowerShell",
  "terminal.integrated.fontFamily": "FiraCode Nerd Font",
  "terminal.integrated.profiles.windows": {
    "Command Prompt": {
      "args": [],
      "icon": "terminal-cmd",
      "path": [
        "${env:windir}\\Sysnative\\cmd.exe",
        "${env:windir}\\System32\\cmd.exe"
      ]
    },
    "Git Bash": {
      "source": "Git Bash"
    },
    "PowerShell": {
      "icon": "terminal-powershell",
      "source": "PowerShell"
    },
    "Windows PowerShell": {
      "path": "C:\\Program Files\\PowerShell\\7\\pwsh.exe"
    }
  },
  "terminal.integrated.scrollback": 10000,
  "vsintellicode.modify.editor.suggestSelection": "automaticallyOverrodeDefaultValue",
  "window.title": "${activeEditorShort}${separator}${rootName} ‚Ä¢ VSCode",
  "workbench.colorTheme": "Min Dark",
  "workbench.iconTheme": "material-icon-theme",
  "workbench.editor.wrapTabs": false,
  "[lua]": {
    "editor.defaultFormatter": "trixnz.vscode-lua"
  },
  "github.copilot.enable": {
    "*": true,
    "yaml": false,
    "plaintext": true,
    "markdown": false,
    "typescriptreact": true
  },
  "JSON-zain.json.autorefresh": true,
  "rojo.pluginManagement": true,
  "errorLens.fontFamily": "Fira Code",
  "errorLens.gutterIconsEnabled": true,
  "errorLens.scrollbarHackEnabled": true,
  "errorLens.statusBarColorsEnabled": true,
  "errorLens.statusBarIconsEnabled": true,
  "errorLens.statusBarMessageEnabled": true,
  "template-string-converter.addBracketsToProps": true,
  "template-string-converter.autoRemoveTemplateString": true,
  "template-string-converter.quoteType": "double",
  "errorLens.excludePatterns": [
    "**/*.{lua}"
  ],
  "files.associations": {
    ".env": "code-text-binary"
  },
  "liveshare.connectionMode": "relay",
  "glualint.linter": "C:\\Users\\callu\\Documents\\glualint-1.21.0-Windows\\glualint.exe",
  "glualint.run": "manual",
  "template-string-converter.validLanguages": [
    "svelte",
    "typescript",
    "javascript",
    "typescriptreact",
    "javascriptreact"
  ],
  "[dart]": {
    "editor.formatOnSave": true,
    "editor.formatOnType": true,
    "editor.rulers": [
      80
    ],
    "editor.selectionHighlight": false,
    "editor.suggest.snippetsPreventQuickSuggestions": false,
    "editor.suggestSelection": "first",
    "editor.tabCompletion": "onlySnippets",
    "editor.wordBasedSuggestions": false
  },
  "explorer.fileNesting.patterns": {
    "*.ts": "${capture}.js",
    "*.js": "${capture}.js.map, ${capture}.min.js, ${capture}.d.ts",
    "*.jsx": "${capture}.js",
    "*.tsx": "${capture}.ts",
    "tsconfig.json": "tsconfig.*.json",
    "package.json": "package-lock.json, yarn.lock",
    "*.d.ts": "${capture}.d.ts",
    ".env.*": ".env.${capture}"
  },
  "explorer.fileNesting.enabled": true,
  "gitlens.detectNestedRepositories": true,
  "js/ts.implicitProjectConfig.target": "ES2022",
  "window.zoomLevel": -3,
  "discord.detailsIdling": "https://callumdev.xyz",
  "discord.lowerDetailsIdling": "https://zarpgaming.com",
  "discord.smallImage": "GitHub - @callumok2004",
  "discord.detailsDebugging": "üß™(DBG) {file_name} ‚Ä¢ {dir_name} ‚Ä¢ {workspace}"
}
