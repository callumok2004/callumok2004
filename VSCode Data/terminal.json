{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "version": 2,
  "final_space": true,
  "blocks": [
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "type": "spotify",
          "style": "powerline",
          "powerline_symbol": "\ue0b0",
          "template": " {{ .Icon }}{{ if ne .Status \"stopped\" }}{{ .Artist }} - {{ .Track }}{{ end }} ",
          "background": "#10602d",
          "foreground": "#ffffff"
        },
        {
          "type": "git",
          "style": "powerline",
          "powerline_symbol": "\ue0b0",
          "template": " {{ .UpstreamIcon }}{{ .HEAD }}{{ if gt .StashCount 0 }} \uf692 {{ .StashCount }}{{ end }} ",
          "properties": {
            "fetch_stash_count": true,
            "fetch_upstream_icon": true
          },
          "background": "#196678",
          "foreground": "#b2cee3"
        },
        {
          "type": "node",
          "style": "powerline",
          "powerline_symbol": "\ue0b0",
          "template": " \ue718 {{ if .PackageManagerIcon }}{{ .PackageManagerIcon }} {{ end }}{{ .Full }} ",
          "background": "#88ce76",
          "foreground": "#000000"
        },
        {
          "type": "shell",
          "style": "powerline",
          "powerline_symbol": "\ue0b0",
          "template": " \ufcb5 {{ .Name }} ",
          "background": "#203248",
          "foreground": "#ffffff"
        },
        {
          "type": "root",
          "style": "powerline",
          "powerline_symbol": "\ue0b0",
          "template": " \uf0e7 ",
          "background": "#ffff66",
          "foreground": "#ffffff"
        },
        {
          "type": "executiontime",
          "style": "diamond",
          "trailing_diamond": "\ue0b4",
          "template": " \ufbab {{ .FormattedMs }}\u2800",
          "properties": {
            "always_enabled": true
          },
          "background": "#5f26c9",
          "foreground": "#ffffff"
        }
      ]
    },
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "type": "text",
          "style": "diamond",
          "leading_diamond": "\ue0c7\u2593",
          "background": "#ffea00",
          "foreground": "#000000"
        },
        {
          "type": "kubectl",
          "style": "powerline",
          "template": " \ufd31{{.Context}}{{if .Namespace}} :: {{.Namespace}}{{end}} ",
          "background": "#ffea00",
          "foreground": "#000000"
        },
        {
          "type": "az",
          "style": "powerline",
          "powerline_symbol": "\ue0b0",
          "template": " \ufd03 {{ .Name }} ",
          "background": "#008AD7",
          "foreground": "#000000"
        }
      ]
    },
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "type": "os",
          "style": "plain",
          "leading_diamond": "\ue0c7\u2593 ",
          "template": " {{ if .WSL }}WSL at {{ end }}{{.Icon}}<#ffea00> | </>",
          "properties": {
            "windows": "\ue70f"
          },
          "foreground": "#26C6DA"
        },
        {
          "type": "path",
          "style": "powerline",
          "trailing_diamond": "\ue0b0",
          "template": "{{ .Path }} ",
          "properties": {
            "style": "full"
          },
          "background": "#2f2f2f",
          "foreground": "#fafafa"
        }
      ],
      "newline": true
    },
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "type": "text",
          "style": "plain",
          "template": " ‚ùØ ",
          "foreground": "#ffea00"
        }
      ]
    }
  ]
}
