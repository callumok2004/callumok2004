{
  "[css]": {
    "editor.defaultFormatter": "sibiraj-s.vscode-scss-formatter"
  },
  "[javascript]": {
    "editor.defaultFormatter": "vscode.typescript-language-features"
  },
  "[php]": {
    "editor.defaultFormatter": "bmewburn.vscode-intelephense-client"
  },
  "[scss]": {
    "editor.defaultFormatter": "sibiraj-s.vscode-scss-formatter"
  },
  "[typescript]": {
    "editor.defaultFormatter": "vscode.typescript-language-features"
  },
  "[typescriptreact]": {
    "editor.defaultFormatter": "vscode.typescript-language-features"
  },
  "deepscan.enable": true,
  "deepscan.ignorePatterns": [
    "**/node_modules/**"
  ],
  "deepscan.ignoreRules": [
    "ASSIGN_SAME_VALUE"
  ],
  "discord.detailsEditing": "üìÅ {file_name} ‚Ä¢ {dir_name} ‚Ä¢ {workspace}",
  "discord.lowerDetailsEditing": "‚úè Line: {current_line}/{total_lines} ‚Ä¢ Col: {current_column}",
  "editor.bracketPairColorization.enabled": true,
  "editor.cursorBlinking": "expand",
  "editor.cursorSmoothCaretAnimation": true,
  "editor.cursorWidth": 5,
  "editor.fontFamily": "'Fira Code'",
  "editor.fontLigatures": true,
  "editor.inlineSuggest.enabled": true,
  "editor.linkedEditing": true,
  "editor.minimap.maxColumn": 100,
  "editor.minimap.scale": 2,
  "editor.minimap.showSlider": "always",
  "editor.suggestSelection": "first",
  "editor.tabSize": 2,
  "emeraldwalk.runonsave": {
    "commands": [
      {
        "match": ".*",
        "isShellCommand": false,
        "cmd": "editor.action.formatDocument"
      }
    ]
  },
  "files.autoSave": "onWindowChange",
  "git.enableSmartCommit": true,
  "importSorter.generalConfiguration.sortOnBeforeSave": true,
  "importSorter.importStringConfiguration.quoteMark": "double",
  "importSorter.importStringConfiguration.tabSize": 2,
  "indentRainbow.colorOnWhiteSpaceOnly": false,
  "javascript.updateImportsOnFileMove.enabled": "always",
  "markdown-preview-enhanced.automaticallyShowPreviewOfMarkdownBeingEdited": true,
  "markdown-preview-enhanced.codeBlockTheme": "monokai.css",
  "php.validate.executablePath": "C:/Users/callu/Documents/php-8.0.9-nts-Win32-vs16-x64/php.exe",
  "security.workspace.trust.untrustedFiles": "open",
  "tabnine.experimentalAutoImports": true,
  "terminal.integrated.defaultProfile.windows": "Windows PowerShell",
  "terminal.integrated.fontFamily": "FiraCode Nerd Font",
  "terminal.integrated.profiles.windows": {
    "Command Prompt": {
      "args": [],
      "icon": "terminal-cmd",
      "path": [
        "${env:windir}\\Sysnative\\cmd.exe",
        "${env:windir}\\System32\\cmd.exe"
      ]
    },
    "Git Bash": {
      "source": "Git Bash"
    },
    "PowerShell": {
      "icon": "terminal-powershell",
      "source": "PowerShell"
    },
    "Windows PowerShell": {
      "path": "C:\\WINDOWS\\System32\\WindowsPowerShell\\v1.0\\powershell.exe"
    }
  },
  "terminal.integrated.scrollback": 10000,
  "vsintellicode.modify.editor.suggestSelection": "automaticallyOverrodeDefaultValue",
  "VSCodeCounter.exclude": [
    "**/.gitignore",
    "**/.vscode/**",
    "**/node_modules/**",
    "**/.git/**"
  ],
  "VSCodeCounter.history": 1,
  "VSCodeCounter.outputAsCSV": false,
  "VSCodeCounter.outputAsText": false,
  "window.title": "${activeEditorShort}${separator}${rootName} ‚Ä¢ VSCode",
  "window.zoomLevel": -2,
  "workbench.colorTheme": "Min Dark",
  "workbench.iconTheme": "material-icon-theme"
}
